    <script>
        function filterAG(ag) {
            const computerProjects = document.getElementById('computer-projects');
            const technikProjects = document.getElementById('technik-projects');
            
            if (ag === 'computer') {
                computerProjects.style.display = 'block';
                technikProjects.style.display = 'none';
            } else if (ag === 'technik') {
                computerProjects.style.display = 'none';
                technikProjects.style.display = 'block';
            }
        }
        
        function showAll() {
            document.getElementById('computer-projects').style.display = 'block';
            document.getElementById('technik-projects').style.display = 'block';
        }
    </script>
